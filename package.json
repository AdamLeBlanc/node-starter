{
  "name": "node-starter",
  "version": "0.0.1",
  "description": "A starter project for node projects using babel for the latest javascript features",
  "main": "index.js",
  "author": "Adam LeBlanc",
  "license": "MIT",
  "engines": {
    "node": ">=8.5.x"
  },
  "scripts": {
    "dev:run": "nodemon --exec babel-node src/index.js",
    "build": "rm -rf build && babel src --out-dir build --source-maps",
    "start": "node build/index.js"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.9.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "nodemon": "^1.12.1",
    "prettier-eslint": "^8.2.1"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5",
    "eslintIntegration": true
  },
  "dependencies": {}
}
